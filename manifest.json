{
  "manifest_version": 2,
  "name": "R.A.M.F. SNR Report",
  "version": "3.9.3",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "A Chrome Extension for R.A.M.F. SNR Report",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon_16.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/report_snr.css"
      ],
      "js": [
        "js/jquery-2.1.3.min.js", 
        "js/plotly-latest.min.js", 
        "js/xlsx.full.min.js", 
        "js/html_js/info.js", 
        "js/html_js/storage.js", 
        "js/html_js/drawchart.js", 
        "js/html_js/app.js",
        "js/html_js/view.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "html/*.html"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage"
  ]
}
